{
  "name": "ai-agent-blockchain",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "clean": "tsc --build --clean",
    "typechain": "npx typechain --target ethers-v5 --out-dir ./src/modules/blockchain/contract-types ./src/modules/blockchain/contracts/*.abi.json",
    "db:add-migration": "npm run clean && npx cross-var npx ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate -d ./src/modules/database/database.orm.config.ts ./src/modules/database/migrations/$npm_config_name",
    "db:migrate": "npx ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d ./src/modules/database/database.orm.config.ts",
    "db:revert": "npx ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert -d ./src/modules/database/database.orm.config.ts",
    "db:drop": "npx ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js schema:drop -d ./src/modules/database.orm.config.ts",
    "db:seed": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-extension/dist/cli/index.js seed -d ./src/modules/database/database.orm.config.ts"
  },
